{
  "name": "musicme-backend",
  "version": "1.0.0",
  "description": "Backend for MusicMe app",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/main.js",
    "start:staging": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --coverage --testResultsProcessor=jest-sonar-reporter"
  },
  "engines": {
    "node": "14.x",
    "npm": "6.14.x"
  },
  "keywords": [],
  "author": "Jérémy Basso",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.21.0",
    "datauri": "^2.0.0",
    "dotenv": "^6.2.0",
    "fastify": "^2.2.0",
    "fastify-formbody": "^3.1.0",
    "fastify-multer": "^1.5.3",
    "fastify-plugin": "^1.5.0",
    "fastify-swagger": "^2.4.0",
    "http-errors": "^1.7.3",
    "js-md5": "^0.7.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.9",
    "node-schedule": "^1.3.2",
    "querystring": "^0.2.0",
    "twilio": "^3.43.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/gapi": "0.0.39",
    "@types/http-errors": "^1.6.2",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/mongoose": "^5.7.12",
    "@types/node": "^10.14.13",
    "@types/node-gcm": "^1.0.0",
    "@types/node-schedule": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "nodemon": "^2.0.7",
    "prettier": "^1.15.3",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.7.0",
    "typescript": "^4.3.4"
  }
}
